.contain {
  width: 100%;
  border-top: 4rpx solid #e3e3e3;
  position: relative;
}

.detail {
  width: 86%;
  height: 100%;
  margin: 0 auto;
  margin-top: 22px;
  border-right: 1rpx solid #f8f8f8;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.detail-top {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  margin-top: 35px;
  color: #2f4f4f;
  letter-spacing: 1px;
}

.detail-top image {
  width: 16px;
  height: 13px;
}

.detail-top text {
  color: #3a3a3a;
  display: inline-block;
  font-size: 13px;
  margin-left: 10rpx;
}

.from {
  width: 90%;
}

.contern-detail {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.section {
  width: 100%;
  height: 36px;
  display: flex;
  align-items: center;
  font-size: 14px;
  margin-top: 12%;
}

.section-three-t {
  width: 100%;
  height: 36px;
  display: flex;
  align-items: center;
  font-size: 14px;
  margin-top: 25px;
}

.section-lef {
  display: flex;
  align-items: center;
  justify-content: center;
}

.section-left {
  font-size: 14px;
  width: 30%;
  height: 38px;
  display: flex;
  align-items: center;
  font-family: PingFang SC;
  color: rgba(51, 51, 51, 1);
  background: rgba(235, 235, 235, 1);
}

.section-right {
  width: 70%;
}

.section-three-right button {
  font-size: 12px;
  color: white;
  background-color: #17c470;
  width: 88px;
  height: 36px;
  line-height: 75rpx;
  box-shadow: 1rpx 3rpx 3rpx 1rpx #b4b2b2;
  margin-left: 42px;
}

.section-three {
  font-size: 14px;
  width: 86px;
  height: 36px;
  display: flex;
  align-items: center;
  font-family: PingFang SC;
  color: rgba(51, 51, 51, 1);
  background: rgba(235, 235, 235, 1);
  justify-content: center;
}

.section-left image {
  width: 13px;
  height: 13px;
  margin-right: 3px;
  margin-left: 7.5px;
}

.one image {
  width: 12px;
  height: 12px;
  margin-left: 14.5px;
  margin-right: 4px;
}

#ipt {
  width: 100%;
  height: 36px;
  font-size: 12px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #ececec;
}

#ipt_exec {
  width: 100%;
  height: 36px;
  font-size: 12px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #ececec;
}

.section-middle-input {
  width: 30%;
}

.btn_getsesion {
  width: 35%;
  margin-left: 5%;
}

.btn_getsesion button {
  color: white;
  width: 100%;
  height: 75rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 80rpx;
  background-color: #17c470;
  font-size: 13px;
  font-weight: 150;
  box-shadow: 1rpx 1rpx #b4b2b2;
}

button::after {
  border: none;
}

button {
  border-radius: 5rpx;
}

.fotter {
  width: 50%;
}

.fotter button {
  color: white;
  height: 75rpx;
  margin: 100rpx 0;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: 150;
  line-height: 80rpx;
  background-color: #17c470;
  font-size: 13px;
  box-shadow: 1rpx 1rpx #b4b2b2;
}
