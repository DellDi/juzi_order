/**index.wxss**/
.container{
  margin: 0;
  padding:0;
  width: 100%;
  height: 100%;
}
.container-top navigator{
  margin: 0;
  padding:0;
  width: 100%;
  height: 200rpx;
}
.container-top image{
  width: 100%;
  height: 100%;
}
.container-content{
  width: 100%;
  height: 100%;
}
.container-content-detail{
  width: 94%;
  height: 95%;
  margin: 0 2% 5% 2%;
  border-top: none;

}
.detail-content{
   width: 81%;
   height: 84%;
   margin: 0 3% 2% 3%;
   padding:5% 6% 6% 6%;
}
.content-top{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-around;
  margin-top: 35rpx;
}
.content-top-image{
  width:130rpx;
  height: 225rpx;
  text-align: center;
  font-size: 35rpx;
}
.content-top-image image{
  width:80rpx;
  height: 80rpx;
  margin-bottom: 30rpx;
}
.one image{
  width:80rpx;
  height: 50rpx;
  margin-top: 20rpx;
}
.content-center{
  width: 500rpx;
  height: 520rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 37rpx;
}
.content-center image{
  width: 500rpx;
  height: 460rpx;
  margin-bottom: 20rpx;
  
}
