/* pages/appointment/assignseat.wxss */
page{  
  background: #f5f5f5;  
}  
/*总体主盒子*/  
.container {  
  position: relative;  
  width: 100%;  
  height: 100%;  
  background-color: #fff;  
}
/*头部*/
.container-top{
  width: 80%;
  height: 100rpx;
  display: flex;
  justify-content:space-between; 
  margin: 0 auto;
}
/*头部左*/
.container-top-left{
  margin-top:25rpx;
  font-size: 28rpx;
  color: black;
  height: 100%;
  width: 300rpx;
  display: flex;
  justify-content:space-between; 
}
.image{
  height: 100%;
  width: 100%;
  display: flex;
  justify-content:row; 
}
.image image{
  width: 50rpx;
  height:50rpx;
  border-radius:50%;
  border: 3rpx solid #ff9933;
  margin-right: 10rpx; 
}
/*头部右*/

.container-top-right image{
  border: 3rpx solid #ff9933;
  margin-top:25rpx;
  width: 50rpx;
  height: 50rpx;
  font-size: 24rpx;

}
.container-content{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  background: #e6e6e6;

}
 /*左侧栏主盒子*/  
.nav_left{  
  /*设置行内块级元素（没使用定位）*/  
  display: inline-block;  
  width: 25%;  
  height: 100%;  
  /*主盒子设置背景色为灰色*/  
  background: #e6e6e6;  
  text-align: center; 
  margin-top:20rpx;
  color:black; 
}  
/*左侧栏list的item*/  
.nav_left .nav_left_items{  
  /*每个高30px*/  
  height: 40px;  
  /*垂直居中*/  
  line-height: 40px;  
  /*再设上下padding增加高度，总高42px*/  
  padding: 6px 0;  
  /*只设下边线*/  
  border-bottom: 1px solid #ffffff;  
  /*文字14px*/  
  font-size: 14px; 
}  
/*左侧栏list的item被选中时*/  
.nav_left .nav_left_items.active{  
  /*背景色变成白色*/  
  background: #ff9933;  
  color: #fff; 
}  
/*右侧栏主盒子*/  
.nav_right{  
  /*右侧盒子使用了绝对定位*/  
  position: absolute;  
  top: 120rpx;  
  right: 0;  
  flex: 1;  
  /*宽度75%，高度占满，并使用百分比布局*/  
  width: 75%;  
  height: 1000px; 
  padding: 30rpx 0;  
  box-sizing: border-box;  
  background: #e6e6e6; 
  /* overflow: hidden; 
   overflow-x: auto;   */
} 
/* .nei{
  width: 1000rpx;
  height: 1000px;
}  */
/*右侧栏list的item*/  
.nav_right .use{  
  /*浮动向左*/  
  float: left;  
  /*每个item设置宽度是33.33%*/  
  width: 25%;  
  height: 85px;  
  text-align: center; 
  background-color: #ff3333; 
  margin-left: 6.2% ;
  margin-bottom:40rpx; 
  border-radius: 20%;
  box-shadow: 1rpx 1rpx 6rpx 2rpx #9d9d9d; 
} 
.nav_right .unuse{  
  /*浮动向左*/  
  float: left;  
  /*每个item设置宽度是33.33%*/  
  width: 25%;  
  height: 85px;  
  text-align: center; 
  background-color: green; 
  margin-left: 6.2% ;
  margin-bottom:40rpx; 
  border-radius: 20%;
  box-shadow: 1rpx 1rpx 6rpx 2rpx #9d9d9d; 
}
.nav_right_items-top{
  display: flex;
  justify-content: row;
  margin-bottom: 30rpx;
}
.nav_right .nav_right_items image{  
  /*被图片设置宽高*/  
  width: 35rpx;  
  height: 35rpx;  
  margin-top: 15px;
  margin-left:10rpx;
  border-radius: 50%;  
}  
.nav_right_items-top-text{
    font-size: 26rpx;
    color: white;
    margin-top: 30rpx;
    margin-left: 12rpx;
}
.nav_right .nav_right_items text{  
  /*给text设成块级元素*/  
  /* display: block;   */  
  font-size: 33rpx;  
  color: white;
  /*设置文字溢出部分为...*/  
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
} 
.nodata_text
{
  color: black;
  font-size: 14px;  
  text-align: center;  
}
