/* pages/queue/getcode.wxss */
.container{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  border-top: 4rpx solid #e3e3e3;
} 
.detail{
  width: 88%;
  height: 100%;
  margin: 0 auto;
}
.section{
  width: 100%;
  height:100rpx;
  display: flex;
  justify-content: row;
  font-size: 35rpx;
  line-height: 100rpx;
  border-bottom: 2rpx solid #bfbfbf;
}
.section-left image{
  width: 40rpx;
  height:50rpx; 
  margin-right: 10rpx;
  margin-top: 25rpx;
}
 input{
  width: 100%;
  height: 100%;
  font-size: 35rpx;

  margin-left: 60rpx; 
}
.section-right-left{
  width:50%;
  height: 100%;
  display: flex;
  justify-content: space-between;
}
.section-right{
  padding: 0 1%;
  width: 100%;
  height: 100%;
  display: flex;

}
.one button{
  font-size:30rpx;
  color: white;
  background-color: #ff9934;
  width:200rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 20rpx;
  margin-right: 30rpx;
}

.fotter button {
  margin-top: 50rpx;
  color: white;
  width: 100%;
  height: 70rpx;
  background-color:#ff9934; 
  font-size: 33rpx;
  margin-bottom:30rpx;
  line-height: 70rpx; 
}
.check-box{
  font-size: 33rpx;
}
