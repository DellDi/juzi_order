/* pages/queue/queue.wxss */
.container{
  width: 94%;
  height: 89%;
  padding:5% 3% 6% 3% ;
}
.container-detail{
  width: 100%;
  height: 97%;
  margin-bottom: 3%;
  background-color:#e6e6e6 ;
}

/**头部**/
.detail-top{
  width: 100%;
  height: 75rpx;
  background-color:#ff9934; 
  display: flex;
 justify-content:row;
}

/**头部左边,右边**/

.top-left{
  display: flex;
  justify-content:row;
  margin-left: 40rpx;
  width: 190rpx;
  height: 100%;
}
.top-left image{
  width: 35rpx;
  height: 35rpx;
  margin-right: 15rpx;
  margin-top: 19rpx;
}
.top-left-text{
  font-size: 30rpx;
  color: white;
  margin-top: 16rpx;
}


/**中间**/

.detail-center{
  width: 93%;
  margin: 40rpx auto;
}
.detail-center-list{
  width: 100%;
  height: 65rpx;
  background-color: #fff;
  border-radius: 30rpx;
  margin-bottom: 40rpx;
  display: flex;
  justify-content: space-around;
  padding-top: 7rpx;
}
.detail-center-list text{
  font-size: 30rpx;
  font-weight: 590;
}
.detail-content-right navigator{
  font-size: 25rpx;
  font-weight: 590;
  color:#fff;
  background-color: #ff9934;
  border-radius: 20rpx;
  padding: 7rpx 18rpx;
  margin-right: -50rpx;
  margin-top: 4rpx;
}

.detail-fotter{
  width: 100%;
  height: 75rpx;
  background-color:#ff9934; 
  display: flex;
  justify-content: center;
  margin-top: 600rpx;

}
.detail-fotter image{
  width: 35rpx;
  height: 35rpx;
  margin-right: 15rpx;
  margin-top: 19rpx;

}
.detail-fotter-text{
  font-size: 30rpx;
  color: white;
  margin-top: 16rpx;
}