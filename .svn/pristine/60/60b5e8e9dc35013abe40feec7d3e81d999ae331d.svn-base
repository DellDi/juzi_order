/* pages/queue/queueres.wxss */
.container{
  width: 93%;
  height: 93%;
  margin: 0;
  padding:3.5% 3.5%;
}
.detail{
  width: 94%;
  height: 92%;
  margin: 0;
  padding:4% 3%;
  background-color: #ff9934;
  
}
.detail-content{
  width: 75%;
  height: 85%;
  margin: 0;
  padding:6% 9%;
  border: 25rpx solid #fff;
  border-radius:30rpx; 
}
.content-circle{
  width: 420rpx;
  height: 400rpx;
  border-radius: 50%;
  background-color: #fff;
  margin: 15rpx auto;
  padding-top:19rpx; 

}
.content-circle-text{
  width: 350rpx;
  height: 350rpx;
  border-radius: 50%;
  border: 15rpx solid #ff9934;
  text-align: center;
  margin: 0 auto;
}
.content-circle-text{
  font-size: 170rpx;
  color: #ff9934;
  line-height: 340rpx;
}
.content-footer{
  margin-top: 100rpx;
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: space-around;
}
.content-footer-left text{
  display: flex;
  justify-content: colunm;
  font-size: 35rpx;
  color: #ffffff;
  margin-top: 10rpx;
}
.text-fotter {
  display: flex;
  justify-content: row;
  
}
.text-fotter text{
  font-size: 38rpx;
  color: #ffffff;
  margin-top: 20rpx;
}
.text-fotter-text{
  font-size: 45rpx;

}
.content-footer-right navigator{
  border: 5rpx solid #fff;
  box-shadow: 1rpx 1rpx 1rpx 1rpx black;
  border-radius: 26rpx;
  padding: 3rpx 8rpx;
  font-size: 30rpx;
  color: white;
  margin-top: 120rpx;

}
.content-footer-right image{
  width: 22rpx;
  height: 22rpx;
}
.fotter{
  margin-top: 120rpx;
  width: 100%;
  height: 90rpx;
  border-radius: 48rpx;
  background-color: #ff9934; 
  text-align: center;
  font-size: 38rpx;
  color: #fff;
  line-height: 90rpx;
}