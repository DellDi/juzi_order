<!--pages/queue/queueres.wxml-->
<view class="container">
    <view class="detail">
        <view class="detail-content">
            <view class="content-circle">
               <view class="content-circle-text">
                  <text>{{queue_no}}</text>
               </view> 
            </view>
            <view class="content-footer">
                <view class="content-footer-left">
                    <text>{{queue_no}}</text>
                    <text>{{table_type}}</text>
                    <view class="text-fotter">
                        <text >前面还有</text>
                        <text class="text-fotter-text">{{num}}桌</text>
                    </view>
                </view>
                <view class="content-footer-right">
                     <navigator url="../index/index">
                        <text>查看菜单</text>
                        <image src="http://mz.djmall.xmisp.cn/files/logo/20161208/148117972563.jpg"></image>
                     </navigator>
                </view>
             </view>
        </view>
     </view>
    <view class="fotter">
       <text> 取消排队（已等待时间{{wait}}分钟）</text>
    </view>
</view>
