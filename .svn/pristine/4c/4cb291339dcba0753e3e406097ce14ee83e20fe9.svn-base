.container{
 width: 100%;
 height: 100%;
 margin: 0;
 padding: 0;
}
.detail{
 width: 88%;
 height: 88%;
 margin: 3%;
 padding:3%;
 background-color:#ff9934;
 border-radius:0 48rpx 48rpx 48rpx;
}

/**头部**/

.detail-top{
  position:relative;
  width: 92%;
  height: 90%;
  padding: 5% 3% 5% 5%;
  background-color: white;
  border-radius:0 48rpx 48rpx 48rpx;  
}
.detail-top-content{
  display: flex;
  justify-content: space-between;
  margin: 2% 0% 2% 2%;
  width: 98%;
  height: 96%; 
}
.circle{
  position: absolute;
  width: 22rpx;
  height: 22rpx;
  border-radius: 50rpx;
  background-color: #ff9934;
  top:15rpx;
  left:15rpx;
}
.detail-top-content-left{
  width: 40%;
  height:100%;
  margin-right: 20rpx;
}
.detail-top-content-left image{
  width: 100%;
  height: 180rpx; 
}
.detail-top-content-right{
  width: 60%;
  height:100%;
}
.list{
  font-size: 30rpx;
  color: black;
  margin-top: 18rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}

/**中间**/
.detail-content{
  margin-bottom: 295rpx;
  width: 100%;
  height: 100%;
}
.detail-content-time,.detail-content-num,.detail-content-name,.detail-content-tel{
  width: 88%;
  height: 73rpx;
  margin-top: 30rpx;
  background-color: #fff;
  color: black;
  border-radius: 48rpx; 
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  line-height: 70rpx;
  padding: 0 6%;
  font-weight: 590;
}
.detail-content-time-right,.detail-content-num-right,.detail-content-name-right,.detail-content-tel-right{
  font-size: 28rpx;
  color: #ff9934;
}
.detail-content-name-right input,.detail-content-tel-right input{
    width: 230rpx;
    height: 100%;
    font-size: 28rpx;
    text-align: right;
}

/**尾部**/

.detail-content-fotter{
  display: flex;
  justify-content: space-between;
  width: 78%;
  height: 85rpx;
  margin: 50rpx auto;
}
.detail-content-fotter-button{
  /* display: flex;
  justify-content: column; */
  width: 38%;
  height: 80rpx;
  background-color: #fff;
  border-radius: 45rpx;
  margin-top:20rpx;
  padding: 0 15rpx;
}
.button{
  display: flex;
  justify-content: row;
  width:225rpx;
  height: 80rpx;
  background-color: #fff;
  border-radius: 45rpx;
  margin-top:20rpx;
  padding: 0 15rpx;
}
.detail-content-fotter-button navigator{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: row;
}
.detail-content-fotter-button image ,.button image{
  width: 40rpx;
  height:40rpx;
  margin-top: 19rpx;
  margin-right:15rpx; 
}
.adr{
  margin-top: 18rpx;
  font-size: 31rpx;
  font-weight: 600;
  color: #ff9934;
}
.one{
  margin-top: 0rpx;
}